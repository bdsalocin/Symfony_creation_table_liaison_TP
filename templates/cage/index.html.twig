{% extends 'base.html.twig' %}

{% block title %}Cage index{% endblock %}

{% block body %}
    <h1>Cage index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Fonctionnalite</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for cage in cages %}
            <tr>
                <td>{{ cage.id }}</td>
                <td>{{ cage.fonctionnalite }}</td>
                <td>
                    <a href="{{ path('app_cage_show', {'id': cage.id}) }}">show</a>
                    <a href="{{ path('app_cage_edit', {'id': cage.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_cage_new') }}">Create new</a>
{% endblock %}
