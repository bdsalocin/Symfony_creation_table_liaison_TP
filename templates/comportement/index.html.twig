{% extends 'base.html.twig' %}

{% block title %}Comportement index{% endblock %}

{% block body %}
    <h1>Comportement index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>TypeComportement</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for comportement in comportements %}
            <tr>
                <td>{{ comportement.id }}</td>
                <td>{{ comportement.typeComportement }}</td>
                <td>
                    <a href="{{ path('app_comportement_show', {'id': comportement.id}) }}">show</a>
                    <a href="{{ path('app_comportement_edit', {'id': comportement.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_comportement_new') }}">Create new</a>
{% endblock %}
