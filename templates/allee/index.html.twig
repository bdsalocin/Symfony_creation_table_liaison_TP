{% extends 'base.html.twig' %}

{% block title %}Allee index{% endblock %}

{% block body %}
    <h1>Allee index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NumeroAllee</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for allee in allees %}
            <tr>
                <td>{{ allee.id }}</td>
                <td>{{ allee.numeroAllee }}</td>
                <td>
                    <a href="{{ path('app_allee_show', {'id': allee.id}) }}">show</a>
                    <a href="{{ path('app_allee_edit', {'id': allee.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_allee_new') }}">Create new</a>
{% endblock %}
